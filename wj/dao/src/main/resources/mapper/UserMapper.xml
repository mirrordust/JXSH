<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="me.gsmr.dao.mapper.UserMapper">
    <sql id="select">
        SELECT * FROM users
    </sql>
    <select id="findById" parameterType="long" resultType="me.gsmr.common.model.entity.account.User">
        <include refid="select"></include>
        WHERE id = #{id}
    </select>
    <select id="findAll" resultType="me.gsmr.common.model.entity.account.User">
        SELECT * FROM users
    </select>
</mapper>